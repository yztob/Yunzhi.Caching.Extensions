{
  "yunzhi": {
    "cache": {
      "defaultProvider": "Yunzhi.Caching.LiteDB.LiteDbProvider, Yunzhi.Caching.LiteDB",
      "parameters": [
        {
          "key": "path",
          "value": "cache.db"
        }
      ],
      "pool": [
        {
          "key": "sessions",
          "provider": "Yunzhi.Common.Configuration.DeltaSection, Yunzhi.Library"
        }
      ]
    },
    "logging": [
      {
        "name": "error",
        "logger": "TextFileLogger",
        "parameters": [
          {
            "key": "Path",
            "value": "logs"
          },
          {
            "key": "FileStyle",
            "value": "error-{0:yyyy-MM-dd}.log"
          },
          {
            "key": "MaxLength",
            "value": 20
          },
          {
            "key": "Head",
            "value": "System Error Log."
          },
          {
            "key": "LogStyle",
            "value": ">>>{Datetime:yyyy-MM-dd HH:mm:ss} @{Source}：\r\n{Descript}\r\n\r\n"
          }
        ]
      }
    ]
  }
}
