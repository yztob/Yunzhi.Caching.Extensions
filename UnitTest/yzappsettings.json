{
  "yunzhi": {
    "cache": {
      "defaultProvider": "Yunzhi.Caching.MemoryProvider, Yunzhi.Library",
      "parameters": [
        {
          "key": "timeout",
          "value": "1"
        },
        {
          "key": "trymax",
          "value": "20"
        }
      ],
      "pool": [
        {
          "key": "lite_cache",
          "provider": "Yunzhi.Caching.LiteDB.LiteDbProvider, Yunzhi.Caching.LiteDB",
          "parameters": [
            {
              "key": "path",
              "value": "li_cache.db"
            }
          ]
        }
      ]
    },
    "logging": [
      {
        "name": "error",
        "logger": "TextFileLogger",
        "parameters": [
          {
            "key": "Path",
            "value": "logs"
          },
          {
            "key": "FileStyle",
            "value": "error-{0:yyyy-MM-dd}.log"
          },
          {
            "key": "MaxLength",
            "value": 20
          },
          {
            "key": "Head",
            "value": "System Error Log."
          },
          {
            "key": "LogStyle",
            "value": ">>>{Datetime:yyyy-MM-dd HH:mm:ss} @{Source}：\r\n{Descript}\r\n\r\n"
          }
        ]
      },
      {
        "name": "info",
        "logger": "TextFileLogger",
        "parameters": [
          {
            "key": "FileStyle",
            "value": "info-{0:yyyy-MM-dd}.log"
          },
          {
            "key": "Head",
            "value": "System Error Log."
          }
        ]
      }
    ]
  }
}
